{
    "name": "express-project",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "migrate": "cd db && ../node_modules/.bin/sequelize db:migrate && cd ..",
        "add-migration": "cd db && read name && node ../node_modules/sequelize-auto-migrations/bin/makemigration.js --name $name && cd ..",
        "seed": "cd db && ../node_modules/.bin/sequelize db:seed:all && cd .."
    },
    "dependencies": {
        "body-parser": "^1.18.2",
        "cookie-parser": "^1.4.3",
        "express": "^4.16.3",
        "express-session": "^1.15.6",
        "morgan": "^1.9.0",
        "mysql2": "^1.5.2",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "pug": "^2.0.1",
        "sequelize": "^4.37.1",
        "sequelize-auto-migrations": "^1.0.2",
        "sequelize-cli": "^4.0.0"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.2",
        "chai": "^4.1.2",
        "eslint": "^4.19.0",
        "eslint-config-google": "^0.9.1",
        "eslint-config-jquery": "^1.0.1",
        "mocha": "^5.0.4",
        "sinon": "^4.4.8"
    }
}
