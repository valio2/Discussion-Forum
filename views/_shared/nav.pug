mixin nav()
    nav.main-nav.navbar
        .container-fluid
            .navbar-header
                a.navbar-brand(href="#")
                    | Discussion Forum
            ul.nav.navbar-nav
                li.active
                    a(href="#")
                        | Home
                li.dropdown
                    a.dropdown-toggle(data-toggle="dropdown" href="#")
                        | Page1
                        span.caret
                        ul.dropdown-menu
                            li
                                a(href="#")
                                    | Page1
                            li
                                a(href="#")
                                    | Page2
                        

